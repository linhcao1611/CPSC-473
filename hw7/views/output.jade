extends layout

block content
	h4 Enter a url:
	form(action="/", method="post")
		input(type="text", name="url")
		input(type="submit", value="submit")

	div 
		h5 Shortener Url: 
			//a(href=shortUrl type="submit")= shortUrl 
			a=shortUrl
		h5 Original Url: 
			a(href="http://"+ longUrl)= longUrl

	div
		h4 Top ten 
			ul
				for elt in list
					li=elt
				